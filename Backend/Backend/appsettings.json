{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "BackendDbContext": "Server={DB_SERVER};Database={DB_DB};User Id={DB_USER};Password={DB_PASSWORD};Encrypt=True;TrustServerCertificate=True;Integrated Security=false",
    "RedisConnection": "localhost:6379"
  },
  "JwtConfig": {
    "Issuer": "http://localhost:3001",
    "Audience": "http://localhost:3001",
    "Key": "",
    "TokenValidityInMinutes": 60
  },
  "FrontendUrl": "http://localhost:3000",
  "Endpoints": {
    "FrontendUrl": "http://localhost:3000"
  },
  "AllowedHosts": "*"
}
